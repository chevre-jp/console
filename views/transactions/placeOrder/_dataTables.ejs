<%
const PROJECT_CREATOR_IDS = (typeof process.env.PROJECT_CREATOR_IDS === 'string')
    ? process.env.PROJECT_CREATOR_IDS.split(',')
    : [];
%>
<div class="material-datatables">
    <div id="datatables_wrapper" class="dataTables_wrapper dt-bootstrap4">
        <div class="row">
            <div class="col-sm-12">
                <form method="post" class="search">
                    <%- include('../../common/master/_hidden') %>

                    <div class="form-row">
                        <div class="form-group bmd-form-group col-md-2">
                            <label for="transactionId" class="bmd-label-static">
                                ID
                            </label>
                            <input type="text" class="form-control" name="id" id="transactionId"
                                placeholder="完全一致" />
                        </div>
                        <div class="form-group bmd-form-group col-md-2">
                            <label for="result[order][orderNumbers]" class="bmd-label-static">
                                注文番号
                            </label>
                            <input type="text" class="form-control" name="result[order][orderNumbers]" id="result[order][orderNumbers]"
                                placeholder="完全一致" />
                        </div>
                    </div>

                    <div class="advanced collapse">
                    </div>
                </form>
            </div>
        </div>
        <div id="list" style="display:none;" class="row">
            <div class="col-sm-12 table-responsive">
                <table id="datatables" class="table table-no-bordered table-hover dataTable dtr-inline" cellspacing="0"
                    width="100%" style="width: 100%;" role="grid" aria-describedby="datatables_info">
                    <thead class="text-primary">
                        <tr>
                            <th>ID</th>
                            <th>start</th>
                            <th>end</th>
                            <th>expires</th>
                            <th>period</th>
                            <th>status</th>
                            <th>tasks</th>
                            <th>agent</th>
                            <th>recipient</th>
                            <th>seller</th>
                            <th>object</th>
                            <th>result</th>
                            <th>actions</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr id="templateRow" data-id="$id$">
                            <td name="id__id">
                                <a href="javascript:void(0)" class="showTransaction" data-id="$id$">
                                    表示
                                </a>
                            </td>
                            <td name="startDate|date:YYYY-MM-DD HH:mm:ssZ">
                                $startDate$
                            </td>
                            <td name="endDate|date:YYYY-MM-DD HH:mm:ssZ">
                                $endDate$
                            </td>
                            <td name="expires|date:YYYY-MM-DD HH:mm:ssZ">
                                $expires$
                            </td>
                            <td name="seconds">
                                $seconds$
                            </td>
                            <td name="status__status">
                                <span class="badge badge-light $status$">$status$</span>
                            </td>
                            <td name="tasksExportationStatus">
                                <span class="badge badge-light">$tasksExportationStatus$</span>
                            </td>
                            <td name="id__agent.typeOf">
                                <a href="javascript:void(0)" class="showAttribute" data-id="$id$" data-attribute="agent">
                                    <span class="badge badge-light">$agent.typeOf$</span>
                                </a>
                            </td>
                            <td name="id__recipient.typeOf">
                                <a href="javascript:void(0)" class="showAttribute" data-id="$id$" data-attribute="recipient">
                                    <span class="badge badge-light">$recipient.typeOf$</span>
                                </a>
                            </td>
                            <td name="id__seller.name.ja">
                                <a href="javascript:void(0)" class="showAttribute" data-id="$id$" data-attribute="seller">
                                    $seller.name.ja$
                                </a>
                            </td>
                            <td name="id__object">
                                <a href="javascript:void(0)" class="showAttribute" data-id="$id$" data-attribute="object">
                                    表示
                                </a>
                            </td>
                            <td name="id__result.order.orderNumber">
                                <a href="javascript:void(0)" class="showAttribute" data-id="$id$" data-attribute="result">
                                    $result.order.orderNumber$
                                </a>
                            </td>
                            <td name="id__actions">
                                <a href="javascript:void(0)" class="showActions" data-id="$id$">
                                    表示
                                </a>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-5">
                <div class="dataTables_info" id="datatables_info" role="status" aria-live="polite">
                </div>
            </div>
            <div class="col-sm-12 col-md-7">
                <div class="dataTables_paginate paging_full_numbers" id="datatables_paginate">
                </div>
                <div id="pager" class="div_pager_parent float-right"></div>
            </div>
        </div>
    </div>
</div>