<div class="material-datatables">
    <div id="datatables_wrapper" class="dataTables_wrapper dt-bootstrap4">
        <div class="row">
            <div class="col-sm-12">
                <form method="post" class="search">
                    <%- include('../common/master/_hidden') %>

                    <div class="form-row">
                        <div class="form-group bmd-form-group col-md-2">
                            <label for="typeOf[$eq]" class="bmd-label-static">
                                タイプ
                            </label>
                            <select class="form-control" name="typeOf[$eq]">
                                <option value="">全て</option>
                                <% Object.keys(ActionType).forEach(function(key) { %>
                                <option value="<%= ActionType[key] %>">
                                    <%= ActionType[key] %>
                                </option>
                                <% }) %>
                            </select>
                        </div>
                        <div class="form-group bmd-form-group col-md-2">
                            <label for="actionStatus[$eq]" class="bmd-label-static">
                                ステータス
                            </label>
                            <select class="form-control" name="actionStatus[$eq]">
                                <option value="">全て</option>
                                <% Object.keys(ActionStatusType).forEach(function(key) { %>
                                <option value="<%= ActionStatusType[key] %>">
                                    <%= ActionStatusType[key] %>
                                </option>
                                <% }) %>
                            </select>
                        </div>
                        <div class="form-group bmd-form-group col-md-2">
                            <label for="object[paymentMethod][accountId][$eq]" class="bmd-label-static">
                                object.paymentMethod.accountId
                            </label>
                            <input class="form-control" placeholder="決済アカウント"
                                name="object[paymentMethod][accountId][$eq]"
                                id="object[paymentMethod][accountId][$eq]" />
                        </div>
                        <div class="form-group bmd-form-group col-md-2">
                            <label for="object[paymentMethod][typeOf][$eq]" class="bmd-label-static">
                                object.paymentMethod.typeOf
                            </label>
                            <input class="form-control" placeholder="決済方法タイプ" name="object[paymentMethod][typeOf][$eq]"
                                id="object[paymentMethod][typeOf][$eq]" />
                        </div>
                        <div class="form-group bmd-form-group col-md-2">
                            <label for="object[paymentMethod][paymentMethodId][$eq]" class="bmd-label-static">
                                object.paymentMethod.paymentMethodId
                            </label>
                            <input class="form-control" placeholder="決済ID"
                                name="object[paymentMethod][paymentMethodId][$eq]"
                                id="object[paymentMethod][paymentMethodId][$eq]" />
                        </div>
                    </div>

                    <div class="advanced collapse">
                        <div class="form-row">
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="agent[typeOf][$eq]" class="bmd-label-static">
                                    agent.typeOf
                                </label>
                                <input type="text" class="form-control" name="agent[typeOf][$eq]"
                                    id="agent[typeOf][$eq]" placeholder="agent.typeOf" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="agent[id][$eq]" class="bmd-label-static">
                                    agent.id
                                </label>
                                <input type="text" class="form-control" name="agent[id][$eq]"
                                    id="agent[id][$eq]" placeholder="agent.id" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="location[identifier][$eq]" class="bmd-label-static">
                                    location.identifier
                                </label>
                                <input type="text" class="form-control" name="location[identifier][$eq]"
                                    id="location[identifier][$eq]" placeholder="location.identifier" />
                            </div>
                        </div>

                        <div class="form-row">
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="object[typeOf][$eq]" class="bmd-label-static">
                                    object.typeOf
                                </label>
                                <input type="text" class="form-control" name="object[typeOf][$eq]"
                                    id="object[typeOf][$eq]" placeholder="object.typeOf" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="object[id][$eq]" class="bmd-label-static">
                                    object.id
                                </label>
                                <input type="text" class="form-control" name="object[id][$eq]"
                                    id="object[id][$eq]" placeholder="object.id" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="object[orderNumber][$eq]" class="bmd-label-static">
                                    object.orderNumber
                                </label>
                                <input type="text" class="form-control" name="object[orderNumber][$eq]"
                                    id="object[orderNumber][$eq]" placeholder="object.orderNumber" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="object[reservationFor][id][$eq]" class="bmd-label-static">
                                    object.reservationFor.id
                                </label>
                                <input type="text" class="form-control" name="object[reservationFor][id][$eq]"
                                    id="object[reservationFor][id][$eq]" placeholder="object.reservationFor.id" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="object[event][id][$eq]" class="bmd-label-static">
                                    object.event.id
                                </label>
                                <input type="text" class="form-control" name="object[event][id][$eq]"
                                    id="object[event][id][$eq]" placeholder="object.event.id" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="object[acceptedOffer][ticketedSeat][seatNumber][$eq]" class="bmd-label-static">
                                    object.acceptedOffer.ticketedSeat.seatNumber
                                </label>
                                <input type="text" class="form-control" name="object[acceptedOffer][ticketedSeat][seatNumber][$eq]"
                                    id="object[acceptedOffer][ticketedSeat][seatNumber][$eq]" placeholder="object.acceptedOffer.ticketedSeat.seatNumber" />
                            </div>
                        </div>

                        <div class="form-row">
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="purpose[typeOf][$eq]" class="bmd-label-static">
                                    purpose.typeOf
                                </label>
                                <input type="text" class="form-control" name="purpose[typeOf][$eq]"
                                    id="purpose[typeOf][$eq]" placeholder="purpose.typeOf" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="purpose[id][$eq]" class="bmd-label-static">
                                    purpose.id
                                </label>
                                <input type="text" class="form-control" name="purpose[id][$eq]"
                                    id="purpose[id][$eq]" placeholder="purpose.id" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="purpose[orderNumber][$eq]" class="bmd-label-static">
                                    purpose.orderNumber
                                </label>
                                <input type="text" class="form-control" name="purpose[orderNumber][$eq]"
                                    id="purpose[orderNumber][$eq]" placeholder="purpose.orderNumber" />
                            </div>
                        </div>

                        <div class="form-row">
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="result[typeOf][$eq]" class="bmd-label-static">
                                    result.typeOf
                                </label>
                                <input type="text" class="form-control" name="result[typeOf][$eq]"
                                    id="result[typeOf][$eq]" placeholder="result.typeOf" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="result[id][$eq]" class="bmd-label-static">
                                    result.id
                                </label>
                                <input type="text" class="form-control" name="result[id][$eq]"
                                    id="result[id][$eq]" placeholder="result.id" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="result[orderNumber][$eq]" class="bmd-label-static">
                                    result.orderNumber
                                </label>
                                <input type="text" class="form-control" name="result[orderNumber][$eq]"
                                    id="result[orderNumber][$eq]" placeholder="result.orderNumber" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="fromLocation[accountNumber][$eq]" class="bmd-label-static">
                                    fromLocation.accountNumber
                                </label>
                                <input type="text" class="form-control" name="fromLocation[accountNumber][$eq]"
                                    id="fromLocation[accountNumber][$eq]" placeholder="fromLocation.accountNumber" />
                            </div>
                            <div class="form-group bmd-form-group col-md-2">
                                <label for="toLocation[accountNumber][$eq]" class="bmd-label-static">
                                    toLocation.accountNumber
                                </label>
                                <input type="text" class="form-control" name="toLocation[accountNumber][$eq]"
                                    id="toLocation[accountNumber][$eq]" placeholder="toLocation.accountNumber" />
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div id="list" style="display:none;" class="row">
            <div class="col-sm-12 table-responsive">
                <table id="datatables" class="table table-no-bordered table-hover dataTable dtr-inline" cellspacing="0"
                    width="100%" style="width: 100%;" role="grid" aria-describedby="datatables_info">
                    <thead class="text-primary">
                        <tr>
                            <th>Type</th>
                            <th>ID</th>
                            <th>Status</th>
                            <th>Start</th>
                            <th>End</th>
                            <th>Agent</th>
                            <th>Recipient</th>
                            <th>Object</th>
                            <th>Purpose</th>
                            <th>Description</th>
                            <th>Instrument</th>
                            <th>Amount</th>
                            <th>From</th>
                            <th>To</th>
                            <th>Result </th>
                            <th>Error</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr id="templateRow" data-id="$id$">
                            <td name="typeOf">
                                <span class="badge badge-light">$typeOf$</span>
                            </td>
                            <td name="id">
                                <a href="javascript:void(0)" class="showAttribute" data-attribute="id" data-id="$id$">
                                    表示
                                </a>
                            </td>
                            <td class="actionStatus" name="actionStatus__actionStatus">
                                <span class="badge badge-light $actionStatus$">$actionStatus$</span>
                            </td>
                            <td name="startDate|date:YYYY-MM-DD HH:mm:ssZ">
                                $startDate$
                            </td>
                            <td name="endDate|date:YYYY-MM-DD HH:mm:ssZ">
                                $endDate$
                            </td>
                            <td name="id__agent__agent.typeOf__agent.name|slice:10">
                                <a href="javascript:void(0)" class="showAttribute" data-attribute="agent"
                                    data-id="$id$">
                                    <span class="badge badge-light">$agent.typeOf$</span><br>
                                    $agent.name$
                                </a>
                            </td>
                            <td name="id__recipient__recipient.typeOf__recipient.name|slice:10">
                                <a href="javascript:void(0)" class="showAttribute" data-attribute="recipient"
                                    data-id="$id$">
                                    <span class="badge badge-light">$recipient.typeOf$</span><br>
                                    $recipient.name$
                                </a>
                            </td>
                            <td name="id__object__objectType__objectId|slice:10">
                                <a href="javascript:void(0)" class="showAttribute" data-attribute="object"
                                    data-id="$id$">
                                    <span class="badge badge-light">$objectType$</span><br>
                                    $objectId$
                                </a>
                            </td>
                            <td name="id__purpose__purposeType__purpose.id|slice:10">
                                <a href="javascript:void(0)" class="showAttribute" data-attribute="purpose"
                                    data-id="$id$">
                                    <span class="badge badge-light">$purposeType$</span><br>
                                    $purpose.id$
                                </a>
                            </td>
                            <td name="id__description__description|slice:10">
                                <a href="javascript:void(0)" title="$description$">$description$</a>
                            </td>
                            <td name="id__instrument__instrumentType">
                                <a href="javascript:void(0)" class="showAttribute" data-attribute="instrument"
                                    data-id="$id$">
                                    <span class="badge badge-light">$instrumentType$</span>
                                </a>
                            </td>
                            <td name="id__amount.value__amount.currency">
                                $amount.value$ $amount.currency$
                            </td>
                            <td name="id__fromLocation.typeOf__fromLocation.name|slice:10__fromLocation.identifier__fromLocation.issuedThrough.id">
                                <span class="badge badge-light">$fromLocation.typeOf$</span><br>
                                $fromLocation.name$ $fromLocation.identifier$
                                <br>$fromLocation.issuedThrough.id$
                            </td>
                            <td name="id__toLocation.typeOf__toLocation.name|slice:10__toLocation.identifier__toLocation.issuedThrough.id">
                                <span class="badge badge-light">$toLocation.typeOf$</span><br>
                                $toLocation.name$ $toLocation.identifier$
                                <br>$toLocation.issuedThrough.id$
                            </td>
                            <td name="id__result__resultType">
                                <a href="javascript:void(0)" class="showAttribute" data-attribute="result"
                                    data-id="$id$">
                                    $resultType$
                                </a>
                            </td>
                            <td name="id__error__errorType">
                                <a href="javascript:void(0)" class="showAttribute" data-attribute="error"
                                    data-id="$id$">
                                    $errorType$
                                </a>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-5">
                <div class="dataTables_info" id="datatables_info" role="status" aria-live="polite">
                </div>
            </div>
            <div class="col-sm-12 col-md-7">
                <div class="dataTables_paginate paging_full_numbers" id="datatables_paginate">
                </div>
                <div id="pager" class="div_pager_parent float-right"></div>
            </div>
        </div>
    </div>
</div>