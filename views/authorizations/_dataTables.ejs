<div class="material-datatables">
    <div id="datatables_wrapper" class="dataTables_wrapper dt-bootstrap4">
        <div class="row">
            <div class="col-sm-12">
                <form method="post" class="search">
                    <%- include('../common/master/_hidden') %>

                    <div class="form-row">
                        <div class="form-group bmd-form-group col-md-2">
                            <label for="code" class="bmd-label-static">
                                コード
                            </label>
                            <input class="form-control" placeholder="コード" name="code"
                                id="code" />
                        </div>
                        <div class="form-group bmd-form-group col-md-4">
                            <label for="validFrom" class="bmd-label-static">
                                有効期間
                            </label>

                            <div class="input-group">
                                <input class="form-control datepicker" placeholder="" maxlength=10
                                    name="validFrom" id="validFrom" value="" />
                                <div class="input-group-append">
                                    <span class="input-group-text">から</span>
                                </div>
                                <input class="form-control datepicker" placeholder="" maxlength=10
                                    name="validThrough" id="validThrough" valufe="" />
                                <div class="input-group-append">
                                    <span class="input-group-text">まで</span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group bmd-form-group col-md-2">
                            <label for="code" class="bmd-label-static">
                                対象タイプ
                            </label>
                            <input class="form-control" placeholder="対象タイプ" name="object[typeOf]"
                                id="object[typeOf]" />
                        </div>
                        <div class="form-group bmd-form-group col-md-2">
                            <label for="code" class="bmd-label-static">
                                対象ID
                            </label>
                            <input class="form-control" placeholder="対象ID" name="object[id]"
                                id="object[id]" />
                        </div>
                    </div>

                    <div class="advanced collapse">
                    </div>
                </form>
            </div>
        </div>
        <div id="list" style="display:none;" class="row">
            <div class="col-sm-12 table-responsive">
                <table id="datatables" class="table table-no-bordered table-hover dataTable dtr-inline" cellspacing="0"
                    width="100%" style="width: 100%;" role="grid" aria-describedby="datatables_info">
                    <thead class="text-primary">
                        <tr>
                            <th>タイプ</th>
                            <th>コード</th>
                            <th>有効期間(から)</th>
                            <th>有効期間(まで)</th>
                            <th>承認対象</th>
                            <th>アクション</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr id="templateRow" data-id="$id$">
                            <td name="typeOf">
                                $typeOf$
                            </td>
                            <td name="code">
                                $code$
                            </td>
                            <td name="validFrom|date:YYYY-MM-DD HH:mm:ssZ">
                                $validFrom$
                            </td>
                            <td name="validUntil|date:YYYY-MM-DD HH:mm:ssZ">
                                $validUntil$
                            </td>
                            <td name="id">
                                <a href="javascript:void(0)" class="showObject" data-id="$id$">
                                    表示
                                </a>
                            </td>
                            <td name="id__actions">
                                <a href="javascript:void(0)" class="showActions" data-id="$id$">
                                    表示
                                </a>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-5">
                <div class="dataTables_info" id="datatables_info" role="status" aria-live="polite">
                </div>
            </div>
            <div class="col-sm-12 col-md-7">
                <div class="dataTables_paginate paging_full_numbers" id="datatables_paginate">
                </div>
                <div id="pager" class="div_pager_parent float-right"></div>
            </div>
        </div>
    </div>
</div>